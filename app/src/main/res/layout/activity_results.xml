<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Results"
    android:background="@drawable/background">

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Results"
        android:textStyle="bold"
        android:textSize="40dp"
        android:gravity="center"
        android:textColor="@color/black"
        android:id="@+id/text"
        />

    <TextView
        android:id="@+id/text1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/text"
        android:text="Class name"
        android:textColor="@color/black"
        android:textSize="20dp" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/text1"
        android:id="@+id/text2"
        android:textColor="@color/black"
        android:text="FE A"
        android:textSize="20dp"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/text2"
        android:id="@+id/text3"
        android:textColor="@color/black"
        android:text="FE B"
        android:textSize="20dp"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/text3"
        android:id="@+id/text4"
        android:textColor="@color/black"
        android:text="FE C"
        android:textSize="20dp"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/text4"
        android:id="@+id/text5"
        android:textColor="@color/black"
        android:text="FE D"
        android:textSize="20dp"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/text5"
        android:id="@+id/text6"
        android:textColor="@color/black"
        android:text="FE E"
        android:textSize="20dp"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@+id/text1"
        android:layout_below="@+id/text"
        android:textSize="20dp"
        android:text="Points"
        android:textColor="@color/black"
        android:layout_marginStart="60dp"
        android:id="@+id/outline1"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@+id/text1"
        android:layout_below="@+id/outline1"
        android:textSize="20dp"
        android:text=""
        android:textColor="@color/black"
        android:layout_marginStart="60dp"
        android:id="@+id/outline2"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@+id/text1"
        android:layout_below="@+id/outline2"
        android:textSize="20dp"
        android:text=""
        android:textColor="@color/black"
        android:layout_marginStart="60dp"
        android:id="@+id/outline3"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@+id/text1"
        android:layout_below="@+id/outline3"
        android:textSize="20dp"
        android:text=""
        android:textColor="@color/black"
        android:layout_marginStart="60dp"
        android:id="@+id/outline4"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@+id/text1"
        android:layout_below="@+id/outline4"
        android:textSize="20dp"
        android:text=""
        android:textColor="@color/black"
        android:layout_marginStart="60dp"
        android:id="@+id/outline5"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@+id/text1"
        android:layout_below="@+id/outline5"
        android:textSize="20dp"
        android:text=""
        android:textColor="@color/black"
        android:layout_marginStart="60dp"
        android:id="@+id/outline6"
        />
</RelativeLayout>